{
	"extends": [
		"tslint:recommended",
		"tslint-eslint-rules"
	],
	"rules": {
		"indent": [true, "tabs", 2],
		"semicolon": [true, "always"],
		"ordered-imports": false,
		"align": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "object-literal-sort-keys": false,
		"no-console": false,
		"trailing-comma": false
	}
}